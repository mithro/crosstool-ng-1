EXPERIMENTAL=y
CT_ARCH_OR1K=y
CT_ARCH="or1k"

# Bare metal compiler
CT_BARE_METAL=y
CT_KERNEL_BARE_METAL=y
CT_KERNEL="bare-metal"
CT_KERNEL_CHOICE_KSYM="BARE_METAL"
CT_KERNEL_BARE_METAL_PKG_KSYM=""

# Get rid of unknown in output
CT_TARGET_ALIAS_SED_EXPR="s/-unknown//"

CT_LIBC_NONE=y
CT_LIBC="none"
CT_LIBC_CHOICE_KSYM="NONE"
CT_THREADS="none"
CT_LIBC_NONE_PKG_KSYM=""

CT_CC_SUPPORT_FORTRAN=n
CT_CC_SUPPORT_ADA=n
CT_CC_SUPPORT_OBJC=n
CT_CC_SUPPORT_OBJCXX=n
CT_CC_SUPPORT_GOLANG=n

# CT_GCC_SRC_RELEASE is not set
CT_GCC_SRC_DEVEL=y
CT_GCC_DEVEL_VCS_git=y
# CT_GCC_DEVEL_VCS_svn is not set
# CT_GCC_DEVEL_VCS_hg is not set
# CT_GCC_DEVEL_VCS_cvs is not set
CT_GCC_DEVEL_VCS="git"
CT_GCC_DEVEL_URL="https://github.com/openrisc/or1k-gcc.git"
CT_GCC_DEVEL_BRANCH="or1k-5.4.0"
CT_GCC_DEVEL_REVISION=""
CT_GCC_DEVEL_SUBDIR=""
CT_GCC_DEVEL_BOOTSTRAP=""
